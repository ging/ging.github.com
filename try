var cite = async function (doi) {
	
	try{
	let example = await Cite.async(doi)

	let output = example.format('bibliography', {
	  lang: 'en-US'
	  
	})

	console.log(output)
	}catch(e){console.error("Could not find ", doi)}

}


for (var d of dois) {
	cite(d)
}


	
